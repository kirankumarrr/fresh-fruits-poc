ary span svg {\r\n  transform: rotate(180deg);\r\n}\r\n.btn-secondary span svg path {\r\n  fill: var(--darkBlue);\r\n}\r\n\r\n.btn-secondary {\r\n  color: var(--darkBlue);\r\n  padding: 6px 16px;\r\n  font-size: 0.875rem;\r\n  min-width: 64px;\r\n  box-sizing: border-box;\r\n  background: var(--white);\r\n  font-family: 'Roboto', 'Helvetica', 'Arial', sans-serif;\r\n  font-weight: 500;\r\n  line-height: 1.75;\r\n  border-radius: 4px;\r\n  letter-spacing: 0.02857em;\r\n  text-transform: uppercase;\r\n  border: 0;\r\n  cursor: pointer;\r\n  min-height: 48px;\r\n  font-weight: bold;\r\n}\r\n\r\n.input-wrapper.active {\r\n  position: relative;\r\n  display: inline-block;\r\n  background: var(--lightBlue);\r\n  border-radius: 50%;\r\n  height: 30px;\r\n  width: 30px;\r\n}\r\n\r\n/* .input-wrapper.active ~ label {\r\n  vertical-align: -webkit-baseline-middle;\r\n} */\r\n\r\n.input-wrapper.active input {\r\n  opacity: 0;\r\n  margin-right: 12.5px;\r\n  margin-top: 4px;\r\n}\r\n.input-wrapper input {\r\n  margin-right: 12.5px;\r\n  margin-top: 4px;\r\n  cursor: pointer;\r\n}\r\n.input-wrapper.active .svg-wrapper {\r\n  display: flex;\r\n  position: absolute;\r\n  top: 49%;\r\n  left: 50%;\r\n  transform: translate(-50%, -50%);\r\n  pointer-events: none;\r\n}\r\n.input-wrapper {\r\n  display: inline;\r\n  height: 30px;\r\n  width: 30px;\r\n}\r\n.svg-wrapper {\r\n  display: none;\r\n}\r\n.svg-wrapper svg path {\r\n  fill: var(--darkBlue);\r\n}\r\n/* COMMON */\r\n\r\n/**LANDING PAGE */\r\n.wrapper {\r\n  height: 100vh;\r\n  padding: 30px 60px;\r\n  position: relative;\r\n  box-sizing: border-box;\r\n}\r\n.surveyHeader h2 {\r\n  font-size: 3rem;\r\n  margin: 0;\r\n}\r\n.surveyStartBody .subHeading {\r\n  font-size: 3.75rem;\r\n  margin: 0;\r\n}\r\n.surveyStartBody .description {\r\n  font-size: 1rem;\r\n  margin: 0 0 20px 0;\r\n  font-weight: bold;\r\n}\r\n\r\n.surveyBodyAbs {\r\n  position: absolute;\r\n  top: 40%;\r\n  left: 50%;\r\n  transform: translate(-50%, -50%);\r\n}\r\n.surveyBody.hide {\r\n  display: none;\r\n}\r\n.surveyBody {\r\n  height: calc(100% - 100px);\r\n  display: flex;\r\n  justify-content: space-between;\r\n  flex-direction: column;\r\n  padding-top: 70px;\r\n}\r\n.survey-body-buttons {\r\n  display: flex;\r\n  justify-content: space-between;\r\n}\r\n\r\n.errorMessage {\r\n  margin: 0;\r\n  padding-top: 5px;\r\n  font-size: 12px;\r\n  color: var(--red);\r\n  font-weight: bold;\r\n  text-transform: capitalize;\r\n}\r\n\r\n/**LANDING PAGE */\r\n\r\n/* SURVEY QA BODY */\r\n.questionsWrapper {\r\n  width: 60%;\r\n}\r\n.headingQuestion {\r\n  font-size: 2.125rem;\r\n}\r\n\r\n.surveyBody ul.option_group {\r\n  display: flex;\r\n  list-style: none;\r\n  width: 260px;\r\n  margin: 0;\r\n  padding: 4px;\r\n  font-size: 1rem;\r\n  justify-content: space-evenly;\r\n  color: var(--orange);\r\n  background: rgba(251, 229, 201, 0.5);\r\n  box-shadow: inset 0px 1px 2px rgba(97, 97, 97, 0.2),\r\n    inset 0px 2px 4px rgba(97, 97, 97, 0.2);\r\n  border-radius: 14px;\r\n}\r\n.surveyBody ul.option_group li.option {\r\n  padding: 4px 12px;\r\n  border-radius: 5px;\r\n  cursor: pointer;\r\n  height: 32px;\r\n  line-height: 24px;\r\n}\r\n.surveyBody ul.option_group li.option.active {\r\n  color: var(--dark);\r\n  background: var(--white);\r\n  border-radius: 12px;\r\n  box-shadow: 0px 1px 2px rgba(191, 113, 15, 0.2),\r\n    0px 2px 4px rgba(191, 113, 15, 0.2);\r\n}\r\n\r\n.radioForm .form-input:first-child {\r\n  margin-bottom: 40px;\r\n  margin-top: 30px;\r\n  min-height: 30px;\r\n}\r\n\r\n.textareaForm {\r\n  width: 500px;\r\n}\r\n.textareaForm textarea {\r\n  width: 497px;\r\n  height: 180px;\r\n  border: 0px;\r\n  margin: 0px;\r\n  padding: 10px;\r\n  font-size: 0.75rem;\r\n  color: var(--dark);\r\n  font-weight: bold;\r\n  box-shadow: inset 3px 2px 4px rgba(0, 0, 0, 0.08);\r\n}\r\n.textareaForm textarea::placeholder {\r\n  font-size: 0.75rem;\r\n  color: var(--dark);\r\n  font-weight: bold;\r\n}\r\n/* SURVEY QA BODY */\r\n\r\n/* Ending Page */\r\n.circle {\r\n  height: 120px;\r\n  width: 120px;\r\n  border-radius: 40%;\r\n  background: var(--white);\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n  margin: 0 auto;\r\n  border: 2px solid #c0c0c0;\r\n}\r\n\r\n.surveyEnd {\r\n  text-align: center;\r\n}\r\n\r\n.subHeadingEnding {\r\n  font-size: 1.25rem;\r\n  color: #818181;\r\n  margin: 0;\r\n}\r\n\r\n.headingEnding {\r\n  font-size: 2.125rem;\r\n  margin: 24px 0 8px 0;\r\n}\r\n/* Ending Page */\r\n"},"lineCount":281}},"error":null,"hash":"a91e9d37a11b44d5573d5d4ebbb87144","cacheData":{"env":{}}}